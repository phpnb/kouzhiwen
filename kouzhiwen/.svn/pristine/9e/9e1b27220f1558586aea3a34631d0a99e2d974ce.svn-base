[ sql ] [ SQL ] INSERT INTO `tpn_class` (`class_type_id` , `name` , `photo` , `number` , `start` , `end` , `price` , `teacher_id` , `describe` , `type` , `power` , `created_at` , `enterprise_id`) VALUES (34 , '云锡控股公司视野计划国际化人才培养项目' , 'http://kouzhiwen.oss-cn-qingdao.aliyuncs.com/video/20181212/20184897985432043.jpg' , 40 , 1544544000 , 1574956800 , 0 , 52 , '<p><strong><span style=\"font-size:19px;font-family:&#39;微软雅黑&#39;,sans-serif;color:#E60416\">一、基本背景</span></strong></p><p><span style=\"font-family: 微软雅黑, sans-serif; text-indent: 37px;\">云锡是一家具有一百三十余年历史的“百年老店”，在新的历史时期，锡产品具有更加广阔的发展前景，如何将这一资源利用好，如何将锡产品推向更广的国际市场，获得更好的效益，树立国际品牌形象，是云锡领导集体尤其重视的问题。</span></p><p><span style=\"font-family: 微软雅黑, sans-serif; text-indent: 37px;\">要走向国际市场，就必须树立成为“国际化”企业的目标，真正理解国际市场，掌握经营国际市场的能力，并学会分析国际市场发展动态掌握经营策略。同时，也要了解国际矿业界在管理、经营上的新理念和新经验，以及新技术在矿业界的应用，知己知彼，为提高产品品质与服务找到参照，为更好的服务好国际市场客户提高相关能力，也为在产品及服务创新上找到更多经验。</span></p><p><strong><span style=\"font-size:19px;font-family:&#39;微软雅黑&#39;,sans-serif;color:#E60416\">二、培训目的</span></strong></p><p style=\"margin-left:0;text-indent:0\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">1、</span><span style=\"font-family: &#39;微软雅黑&#39;,sans-serif\">了解西方商业文化、价值观以及矿业界相关法律及商业规则。</span></p><p style=\"margin-left:0;text-indent:0\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">2、</span><span style=\"font-family: &#39;微软雅黑&#39;,sans-serif\">掌握必要的国际商务礼仪。</span></p><p style=\"margin-left:0;text-indent:0\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">3、</span><span style=\"font-family: &#39;微软雅黑&#39;,sans-serif\">学习世界先进矿业企业的经营管理经验，特别是科技应用及管理方面的经验。</span></p><p style=\"margin-left:0;text-indent:0\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">4、</span><span style=\"font-family: &#39;微软雅黑&#39;,sans-serif\">学习中国矿业企业走向国际市场的经验，以及学习我国对走出去的相关政策。</span></p><p style=\"margin-left:0;text-indent:0\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">5、</span><span style=\"font-family: &#39;微软雅黑&#39;,sans-serif\">学习开辟海外市场、经营好海外市场的经验。</span></p><p style=\"margin-left:0;text-indent:0\"><span style=\"font-family: &#39;微软雅黑&#39;,sans-serif\"><strong style=\"white-space: normal;\"><span style=\"font-size: 19px; font-family: 微软雅黑, sans-serif; color: rgb(230, 4, 22);\">三、师资介绍</span></strong></span></p><p style=\"margin-left:0;text-indent:0\"><span style=\"font-family: &#39;微软雅黑&#39;,sans-serif\"><strong style=\"white-space: normal;\"><span style=\"font-size: 19px; font-family: 微软雅黑, sans-serif; color: rgb(230, 4, 22);\"></span></strong></span></p><table><tbody><tr class=\"firstRow\"><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><p><img src=\"http://admin.kzwchina.com/uploads/ueditor/20181212/1544606725530424.png\" title=\"1544606725530424.png\" alt=\"image.png\"/></p><p><strong><span style=\"font-size:19px;font-family:黑体;color:#2E74B5\">田正仁</span></strong></p></td><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><span style=\"font-size: 16px;font-family: 微软雅黑, sans-serif\">Jerry Tien</span> <span style=\"font-size:16px;font-family:&#39;微软雅黑&#39;,sans-serif\">（田正仁）教授从事采矿现场及教学工作已逾44年，其中包括现场实际工作14年，澳洲墨尔本莫纳什大学工学院采矿系原系主任。之前曾在密苏里大学罗拉分校采矿系执教28年。该校采矿工程系美国排前三名，创建于清同治九年 (1870), 采矿工程为该校首创专业，毕业校友多年来在世界采矿工程界声誉卓著。曾任山西亚美大陆煤炭公司副总裁，该公司中美合资，辖下大宁煤矿年产四百万吨优质无烟煤。曾任加拿大华阳能源公司董事，该公司曾经在内蒙古霍林河经营露天煤矿及在新疆。田教授是美国联邦技术研究委员会中六名专家委员之一。该委员会因布什总统所签署之2006年矿业安全法 (MINER Act of 2006) 专门成立，成员分别为参众两院领袖，内政部长及劳⼯部长正式委派。 田教授为劳工部长赵小兰任命两位成员之一，主要研究井下皮带巷道风流在工作面使用之可行性与输送皮带材料的可燃性能，为美国国会近期修改煤矿安全法参考。曾开授矿业经济，井下通风与安全，井工开采等课目，在密苏里大学执教期间曾兼任该系远程教学中心主任五年 (Director, Online Master of Engineering Program) ，任美国西部矿安陪训中心主任 (Director, Western Mining Safety &amp; Health Training and Translation Center) 六年。</span></td></tr><tr><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><p><img src=\"http://admin.kzwchina.com/uploads/ueditor/20181212/1544606804516276.png\" title=\"1544606804516276.png\" alt=\"image.png\" width=\"149\" height=\"196\"/></p><p><strong><span style=\"font-size:19px;font-family:黑体;color:#2E74B5\">祁娜</span></strong></p></td><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">北京叩之问企业管理研究院职业素养研究专家</span></p><p style=\"text-indent:32px;line-height:29px\"><strong><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">曾任：</span></strong></p><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">摩托罗拉(中国)电子有限公司企业内训师</span></p><p style=\"text-indent:32px;line-height:29px\"><strong><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">获得：</span></strong></p><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">北京大学心理系研究生证书</span></p><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">加拿大多伦多大学高效焦点解决(SFBC)心理教练证书</span></p><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">中国社工联合会心理健康委员会注册心理咨询师证书</span></p><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">中国职业经理人协会培训专业委员会委员证书</span></p><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">美国人力资源协会(SHRM)会员证书</span></p><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">国家注册二级心理咨询师证书</span></p><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">2016</span><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">、2017年中国“百强职业培训师”奖项</span></p><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">中国“行政管理课程卓越培训师”奖项</span></p><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">中国“行政关系管理卓越讲师”奖项</span></p><p style=\"text-indent:32px;line-height:29px\"><strong><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">受聘：</span></strong></p><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">北大、清华、浙大、武大总裁班特聘讲师</span></p><p style=\"text-indent:32px;line-height:29px\"><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">SMI</span><span style=\"font-family:&#39;微软雅黑&#39;,sans-serif\">、施耐德、三星、LG商学院特聘讲师</span></p><span style=\"font-size:16px;font-family:&#39;微软雅黑&#39;,sans-serif\">浙江省金融行业职业素养特聘讲师</span></td></tr><tr><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><p><img src=\"http://admin.kzwchina.com/uploads/ueditor/20181212/1544606843573320.png\" title=\"1544606843573320.png\" alt=\"image.png\"/></p><p><strong><span style=\"font-size:19px;font-family:黑体;color:#2E74B5\">Prof. Serkan Saydam</span></strong></p></td><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><span style=\"font-size: 16px;font-family: 微软雅黑, sans-serif\">Saydam</span><span style=\"font-size: 16px;font-family: 微软雅黑, sans-serif\">教授毕业于土耳其并获得工学学士、理学硕士和博士学位，并在南非约翰内斯堡Witwatersrand 大学完成了博士后。Saydam教授于2006年于新南威尔士大学担任采矿工程学院高级讲师，于2011年晋升为副教授。他目前主持3个ARC澳大利亚国家重点项目和5个ACARP研究基金，价值超过450万美金，用于地面控制和采矿系统领域。此外他还与美国宇航局实验室和肯尼迪航空中心建立研究合作关系，研究重点是解决当前采矿业面临的需求及未来的挑战。因为随着社会、环境、健康和安全标准的严格化，采矿环境变得更加苛刻。Saydam教授研究的领域包括地面控制、矿山规划设计、新采矿方法和采矿。</span></td></tr><tr><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><p><img src=\"http://admin.kzwchina.com/uploads/ueditor/20181212/1544606862777789.png\" title=\"1544606862777789.png\" alt=\"image.png\"/></p><p><strong><span style=\"font-size:19px;font-family:黑体;color:#2E74B5\">邢厚媛</span></strong></p></td><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><span style=\"font-size: 16px;font-family: 微软雅黑, sans-serif\">商务部国际贸易经济合作研究院跨国经营研究部主任、海外投资研究中心主任;国务院授予的有突出贡献专家，享受政府特殊津贴;&quot;新世纪百千万人才工程&quot;国家级人选;中国国际经济合作学会理事;中国国际贸易学会理事;联合国和国家级课题主持人。曾经参与中央、相关部委重大课题以及十五和十一五发展规划的相关研究工作;并承担了国际组织、外国政府、国内外多家大中型企业发展战略和规划研究。</span></td></tr><tr><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><p><img src=\"http://admin.kzwchina.com/uploads/ueditor/20181212/1544606875487533.png\" title=\"1544606875487533.png\" alt=\"image.png\"/></p><p><strong><span style=\"font-size:19px;font-family:黑体;color:#2E74B5\">张呈国</span></strong></p></td><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><span style=\"font-size: 16px;font-family: 微软雅黑, sans-serif\">新南威尔士大学矿业与能源工程学院讲师（最年轻的），国际煤炭研究联盟秘书长。张博士对澳大利亚先进的矿业管理和运作有着深入的认知，尤其对于中资企业在澳洲的矿产合作有着全面的了解。张博士与澳大利亚新南威尔士州和昆士兰州的多达10余家矿山企业有密切的合作关系。张博士现参与和领导的科研总经费达120余万澳元，有着丰富的纵向横向科研项目经历。曾任新南威尔士大州学者协会秘书长等职务。</span></td></tr><tr><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><p><img src=\"http://admin.kzwchina.com/uploads/ueditor/20181212/1544606890348954.png\" title=\"1544606890348954.png\" alt=\"image.png\"/></p><p><strong><span style=\"font-size:19px;font-family:黑体;color:#2E74B5\">陈德芳</span></strong></p></td><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><span style=\"font-size:16px;font-family:&#39;微软雅黑&#39;,sans-serif\">万宝矿产有限公司总经理</span></td></tr><tr><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><p><img src=\"http://admin.kzwchina.com/uploads/ueditor/20181212/1544606916996151.png\" title=\"1544606916996151.png\" alt=\"image.png\"/></p><p><strong><span style=\"font-size:19px;font-family:黑体;color:#2E74B5\">范厚华</span></strong></p></td><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><span style=\"font-size:16px;font-family: &#39;微软雅黑&#39;,sans-serif\">1996</span><span style=\"font-size:16px;font-family:&#39;微软雅黑&#39;,sans-serif\">年底加入华为，30年企业产品开发、市场战略制定、市场拓展、客户定位、公司治理、企业国际化、全球化战略制定与区域市场领导者; 17年华为国内外市场、子公司管理、国际化、公司战略实践者、贡献者; 3年华为战略与实践研究、华为管理实践者;曾任华为海外片区副总裁、区域总裁、国内外子 公司CEO/COO、IFS变革副总裁/OTC变革总监、部门部长/副部长。</span></td></tr><tr><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><p><img src=\"http://admin.kzwchina.com/uploads/ueditor/20181212/1544606929156919.png\" title=\"1544606929156919.png\" alt=\"image.png\"/></p><p><strong><span style=\"font-size:19px;font-family:黑体;color:#2E74B5\">常云波</span></strong></p></td><td width=\"371\" valign=\"top\" style=\"word-break: break-all;\"><span style=\"font-size:16px;font-family:&#39;微软雅黑&#39;,sans-serif\">中国交通建设股份有限公司海外部副总经理。分管拉美地区业务开发和海外投资业务。自1988年开始从事国际工程承包业务市场开发和海外工程项目管理工作。曾在中东地区常驻12年，在马来西亚常驻2年，在30多个国家从事过业务开发和商务工作。曾任中国港湾工程有限责任公司副总经济师、市场开发部总经理、槟城二桥项目执行总经理等职务，具有丰富的国际工程承包业务开发和工程管理经验。</span></td></tr></tbody></table><p style=\"margin-left:0;text-indent:0\"><span style=\"font-family: &#39;微软雅黑&#39;,sans-serif\"><strong style=\"white-space: normal;\"><span style=\"font-size: 19px; font-family: 微软雅黑, sans-serif; color: rgb(230, 4, 22);\"></span></strong></span><br/></p><p><br/></p>' , 1 , 1 , 1544607184 , 13) [ RunTime:0.003255s ]
