@import url(http://fonts.googleapis.com/css?family=Cabin:400,600,700);
@import url(https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css);
body {
  font: 400 16px 'Cabin', 'Helvetica Neue', 'Arial', sans-serif;
  line-height: 26px;
}
h1 {
  font: 700 40px 'Cabin', 'Helvetica Neue', 'Arial', sans-serif;
  color: #333;
  margin: 0 0 30px 0;
}
a {
  color: #EA5C49;
}
a:hover {
  color: #EA5C49;
}
pre {
  background: url('../img/code-bg.png') #ffffff !important;
  padding: 0 30px 0 30px !important;
  box-shadow: 0 0 0 1px rgba(0, 0, 0, 0.15), 0 1px 4px rgba(0, 0, 0, 0.2) !important;
  border-radius: 2px !important;
  display: block;
  color: #555 !important;
  margin: 30px 0 30px 0 !important;
  border: none !important;
  position: relative;
}
pre.html:before,
pre.js:before {
  display: inline-block;
  font: 700 9px 'Cabin', 'Helvetica Neue', 'Arial', sans-serif;
  background-color: rgba(0, 0, 0, 0.2);
  border-radius: 0 0 0 3px;
  color: #fff;
  padding: 3px 6px;
  position: absolute;
  right: 0;
  top: 0;
}
pre.html:before {
  content: 'HTML';
}
pre.js:before {
  content: 'JS';
}
pre code {
  background: transparent !important;
  padding: 0 !important;
  margin: 0 !important;
  overflow-y: hidden;
  overflow-x: auto;
  max-height: 700px;
  width: 100%;
}
pre code .opacity {
  opacity: 0.4;
}
.text-center {
  text-align: center;
}
a.button {
  padding: 14px 36px;
  border-radius: 4px;
  background-color: #EA5C49;
  color: #fff;
  text-decoration: none;
  display: inline-block;
  font: 700 16px 'Cabin', 'Helvetica Neue', 'Arial', sans-serif;
  -webkit-transition: all 0.2s;
  -moz-transition: all 0.2s;
  -ms-transition: all 0.2s;
  -o-transition: all 0.2s;
  transition: all 0.2s;
}
a.button:hover {
  background-color: transparent;
  box-shadow: inset 0 0 0 1px #EA5C49;
  color: #EA5C49;
}
ul.nav {
  margin: 0;
  padding: 0;
  font-size: 0;
  text-align: center;
  box-shadow: 0 1px 0 rgba(0, 0, 0, 0.1);
}
ul.nav li {
  padding: 0;
  margin: 0;
  display: inline-block;
  font: 700 14px 'Cabin', 'Helvetica Neue', 'Arial', sans-serif;
}
ul.nav li a {
  color: rgba(0, 0, 0, 0.4);
  display: inline-block;
  width: 100%;
  padding: 30px 30px;
}
ul.nav li a.selected,
ul.nav li a:hover {
  color: rgba(0, 0, 0, 0.8);
  background-color: transparent;
}
.jalendar-input input {
  padding: 10px 15px;
  font: 15px 'Helvetica Neue', 'Arial';
  border-radius: 3px;
  box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.1), 0 0 0 1px rgba(0, 0, 0, 0.1);
  outline: none;
  border: none;
  width: 280px;
  cursor: pointer;
}
.container.white {
  background-color: #fff;
  margin-top: 40px;
  border-radius: 5px;
  box-shadow: inset 0 0 0 1px rgba(0, 0, 0, 0.1);
  padding: 40px 60px 1px 60px;
}
.header {
  background-color: #EA5C49;
}
.header.header-mini > .container {
  padding: 0;
}
.header > .container {
  padding: 80px 0 150px 0;
}
.header .logo {
  border-bottom: 1px solid rgba(255, 255, 255, 0.2);
  padding: 40px 0;
  margin: 0;
}
.header p.mini-desc {
  font: 400 30px 'Cabin', 'Helvetica Neue', 'Arial', sans-serif;
  line-height: 48px;
  color: #fff;
  padding: 40px 0;
  letter-spacing: 1px;
  margin: 0;
}
.footer {
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  padding: 50px 0;
  text-align: center;
  margin: 50px 0 0 0;
}
.footer .sharing {
  margin: 0 0 30px 0;
}
.footer .sharing > div {
  display: inline-block;
  margin: 0 20px;
}
.features {
  padding: 100px 0 150px 0;
  text-align: center;
  background: url(../img/features-bg.jpg) no-repeat top center;
  background-size: 100% auto;
}
.features .title {
  font: 700 26px 'Cabin', 'Helvetica Neue', 'Arial', sans-serif;
  color: #333;
}
.features p {
  font: 16px/26px 'Helvetica Neue', 'Arial';
  color: #666;
  padding: 20px 0 0 0;
}
.demos {
  margin-top: -110px;
}
.demos .see-demos {
  text-align: center;
  padding: 20px 0;
  font: 700 14px 'Cabin', 'Helvetica Neue', 'Arial', sans-serif;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
}
.demos.detail {
  margin-top: 0 !important;
  margin-left: -59px;
  margin-right: -59px;
}
.demos.detail .container {
  padding-left: 60px;
  padding-right: 60px;
}
.container-fluid .tab-content {
  padding-top: 80px !important;
  padding-bottom: 80px !important;
}
.container-fluid .tab-content h1 {
  font: 700 40px 'Cabin', 'Helvetica Neue', 'Arial', sans-serif;
  margin: 0 0 30px 0;
  padding: 0 0 20px 0;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.container-fluid .tab-content h1 small {
  font-size: 40px;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.2);
}
.container-fluid .tab-content p {
  margin: 10px 0;
  padding: 0;
  font: 400 14px 'Cabin', 'Helvetica Neue', 'Arial', sans-serif;
  line-height: 24px;
  color: #333;
}
.container-fluid .tab-content:nth-child(1),
.tab a:nth-child(1) {
  background-color: #D9FFF7;
}
.container-fluid .tab-content:nth-child(2),
.tab a:nth-child(2) {
  background-color: #FFDDD9;
}
.container-fluid .tab-content:nth-child(3),
.tab a:nth-child(3) {
  background-color: #EFFFD9;
}
.container-fluid .tab-content:nth-child(4),
.tab a:nth-child(4) {
  background-color: #EADAED;
}
/* tab */
.tab {
  display: block;
  text-align: center;
  font-size: 0;
}
.tab > a {
  display: inline-block;
  padding: 14px 36px;
  position: relative;
  border-radius: 3px 3px 0 0;
  color: rgba(0, 0, 0, 0.5);
  text-decoration: none;
  margin: 0 1px;
  font: 700 16px 'Cabin', 'Helvetica Neue', 'Arial', sans-serif;
}
.tab > a:hover {
  box-shadow: inset 0 -4px 0 0 rgba(0, 0, 0, 0.05);
}
.tab > a.selected {
  color: rgba(0, 0, 0, 0.8);
}
.tab > a.selected:hover {
  box-shadow: none;
}
.tab > a small {
  font: 400 12px 'Cabin', 'Helvetica Neue', 'Arial', sans-serif;
}
.tab-container {
  display: inline-block;
  width: 100%;
}
.tab-container .tab-content {
  display: none;
}
.tab-container .tab-content.selected {
  display: block;
}
/* tab #end */
.row.continue {
  padding: 80px 0;
}
