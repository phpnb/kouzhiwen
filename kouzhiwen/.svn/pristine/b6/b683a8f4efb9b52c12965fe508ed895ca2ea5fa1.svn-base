}
#login {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: url(../images/bg2.jpg) no-repeat;
  background-size: 100%;
}
.bgz {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: #000;
  opacity: 0.2;
  filter: alpha(opacity=20);
}
#login_box_bg {
  position: absolute;
  margin: auto;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 300px;
  height: 240px;
  opacity: 0.4;
  filter: alpha(opacity=40);
  background: #000;
  border-radius: 5px;
}
#login_box {
  position: absolute;
