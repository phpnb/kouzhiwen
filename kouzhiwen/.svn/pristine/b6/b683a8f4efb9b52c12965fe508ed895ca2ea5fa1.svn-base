}
.checkbox-inline,
.radio-inline {
  cursor: pointer;
  padding-top: 2px;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
#top {
